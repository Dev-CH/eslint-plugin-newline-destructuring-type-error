{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "newline-destructuring"
    ],
    "rules": {
        "newline-destructuring/newline": [
            "error",
            {
                "items": 1
            }
        ]
    }
}
